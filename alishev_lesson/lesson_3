public class lesson_3 {
    public static main(String[] args) {
        int x = 5;
        String s = "Hello";
        String space = " ";
        String name = "Bob";
        System.out.println(s+space+name);
        System.out.println("Hello" + " " + "John");
        System.out.println("My number is " + x);
    }
}
